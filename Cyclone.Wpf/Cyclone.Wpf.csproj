<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net48;net8.0-windows</TargetFrameworks>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
	  <LangVersion>13.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyVersion>1.25.3.28</AssemblyVersion>
    <FileVersion>1.25.3.28</FileVersion>
	  <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Cyclone.Wpf</Title>
    <Copyright>CopyRight@Cyclone.Com</Copyright>
    <RepositoryUrl>https://github.com/yangf85/Cyclone.Wpf</RepositoryUrl>
    <Description>Cyclone.Wpf</Description>
	  <PackageOutputPath>E:\YangF\Code\Nugets</PackageOutputPath>

	  <Version>$(AssemblyVersion)</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="Resources\**" />
    <Page Remove="Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Controls\Card\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ToastNotifications.Net" Version="2.6.0" />
  </ItemGroup>

</Project>
