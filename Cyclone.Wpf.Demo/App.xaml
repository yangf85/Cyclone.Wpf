<Application
    x:Class="Cyclone.Wpf.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Cyclone.Wpf.Demo"
    StartupUri="ShellWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Cyclone.Wpf;component/Themes/Generic.xaml" />
                <ResourceDictionary>
                    <FontFamily x:Key="IconFont">
                        pack://application:,,,/Cyclone.Wpf.Demo;component/Assets/#iconfont
                    </FontFamily>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
